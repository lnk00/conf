{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"alignment": "right",
			"segments": [
				{
					"background": "#dcdfe7",
					"foreground": "#8389a3",
					"leading_diamond": "\ue0b6",
					"properties": {
						"style": "folder"
					},
					"style": "diamond",
					"template": "\ue5ff {{ .Path }}",
					"trailing_diamond": "\ue0b4",
					"type": "path"
				},
				{
					"background": "#dcdfe7",
					"foreground": "#3f84a6",
					"leading_diamond": " \ue0b6",
					"properties": {
						"branch_icon": ""
					},
					"style": "diamond",
					"template": "{{ .HEAD }}",
					"trailing_diamond": "\ue0b4",
					"type": "git"
				}
			],
			"type": "rprompt"
		},
		{
			"alignment": "left",
			"segments": [
				{
					"background": "#8389a3",
					"foreground": "#fcfcfc",
					"leading_diamond": "\ue0b6",
					"style": "diamond",
					"template": "{{ .OS }} \u2301",
					"trailing_diamond": "\ue0b4 ",
					"type": "session"
				}
			],
			"type": "prompt"
		}
	],
	"transient_prompt": {
		"background": "transparent",
		"foreground": "#8389a3",
		"template": "\u23fa "
	},
	"version": 2
}
