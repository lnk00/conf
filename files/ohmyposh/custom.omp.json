{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "transient_prompt": {
    "template": "\u23fa ",
    "foreground": "#888888",
    "background": "transparent"
  },
  "blocks": [
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "folder"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue5ff {{ .Path }}",
          "foreground": "#b8b8b8",
          "background": "#4a4a4a",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "branch_icon": "",
            "cache_duration": "none"
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ .HEAD }}",
          "foreground": "#b8b8b8",
          "background": "#4a4a4a",
          "type": "git",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "{{ .OS }} \u2301",
          "foreground": "#a8a8a8",
          "background": "#2d2d2d",
          "type": "session",
          "style": "diamond"
        }
      ]
    }
  ],
  "version": 3
}
